    "devDependencies": {
        "@types/jest": "^26.0.24",
        "@types/uuid": "^8.3.1",
        "jest": "^27.0.6",
        "ts-jest": "^27.0.3",
        "ts-loader": "^6.0.2",
        "typescript": "^4.0.2",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2",
